
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Admin Area</title>
    <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link type="text/css" rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.2/css/materialize.min.css" />
    <link type="text/css" rel="stylesheet" href="/assets/css/custom.css?v=0.0.1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>
<body class="grey lighten-1">
    <div class="section no-pad-bot" id="index-banner">
        <div class="container">
            <div class="section step0">
                <h2 class="header center black-text">INSERISCI LA PASSWORD PER ACCEDERE</h2>

                <div class="row">
                    <form class="col s12 center-align">
                        <div class="row">
                            <div class="input-field">
                                <input id="password" type="password" class="validate">
                                <label for="password">Password</label>
                            </div>
                        </div>
                        <button class="btn waves-effect waves-light" type="submit" name="action">
                            Submit
                            <i class="material-icons right">send</i>
                        </button>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.2/js/materialize.min.js"></script>
</body>
</html>
